<div class="sidebar" id="sidebar">
	<script type="text/javascript">
		try {
			ace.settings.check('sidebar', 'fixed')
		} catch (e) {
		}
	</script>
	<div class="sidebar-shortcuts" id="sidebar-shortcuts">
		<div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
			<button class="btn btn-success">
				<i class="icon-signal"></i>
			</button>
			<button class="btn btn-info">
				<i class="icon-pencil"></i>
			</button>
			<button class="btn btn-warning">
				<i class="icon-group"></i>
			</button>
			<button class="btn btn-danger">
				<i class="icon-cogs"></i>
			</button>
		</div>
		<div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
			<span class="btn btn-success"></span> <span class="btn btn-info"></span>

			<span class="btn btn-warning"></span> <span class="btn btn-danger"></span>
		</div>
	</div>
	<!-- #sidebar-shortcuts -->

	<ul class="nav nav-list">
		<li class="open"><a href="#" class="dropdown-toggle"> <i
				class="icon-tag"></i> <span class="menu-text"> 调度中心 </span> <b
				class="arrow icon-angle-down"></b>
		</a>
			<ul class="submenu" style="display: block;">
				<li class="#if($path1=='task')active#end"><a
					href="${rc.contextPath}/task"> <i></i> 新建任务
				</a></li>
				<li class="#if($path1=='mytask')active#end"><a
					href="${rc.contextPath}/mytask"> <i></i> 我的任务
				</a></li>
				<li class="#if($path1=='workflow')active#end"><a
                    href="${rc.contextPath}/workflow"> <i></i> 我的工作流
                </a></li>
			</ul></li>
		#if($userContext.getUserContext().username == "admin")<li class="#if($path1=='agent')active#end"><a
			href="${rc.contextPath}/agent"> <i class="icon-dashboard"></i> <span
				class="menu-text"> Agent 管理 </span>
		</a></li>
		<li class="#if($path1=='user')active#end"><a
			href="${rc.contextPath}/user"> <i class="icon-text-width"></i> <span
				class="menu-text"> 用户管理 </span>
		</a></li>#end
	</ul>
	<!-- /.nav-list -->
	<div class="sidebar-collapse" id="sidebar-collapse">
		<i class="icon-double-angle-left" data-icon1="icon-double-angle-left"
			data-icon2="icon-double-angle-right"></i>
	</div>
	<script type="text/javascript">
		try {
			ace.settings.check('sidebar', 'collapsed')
		} catch (e) {
		}
	</script>
</div>